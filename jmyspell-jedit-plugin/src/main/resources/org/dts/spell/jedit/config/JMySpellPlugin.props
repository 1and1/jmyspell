### Properties used by jEdit
plugin.org.dts.spell.jedit.JMySpellPlugin.activate = startup

plugin.org.dts.spell.jedit.JMySpellPlugin.name = JMySpell
plugin.org.dts.spell.jedit.JMySpellPlugin.author = DreamTangerine
plugin.org.dts.spell.jedit.JMySpellPlugin.version = 0.0.1
plugin.org.dts.spell.jedit.JMySpellPlugin.docs = org/dts/spell/jedit/config/index.html
plugin.org.dts.spell.jedit.JMySpellPlugin.jars = jmyspell-core.jar jmyspell-swing.jar

### These tell jEdit what JDK and jEdit version we require

plugin.org.dts.spell.jedit.JMySpellPlugin.depend.0 = jedit 04.02.02.00
plugin.org.dts.spell.jedit.JMySpellPlugin.depend.1 = jdk 1.5


### Menu
plugin.org.dts.spell.jedit.JMySpellPlugin.menu =jmyspell.spell-check \
jmyspell.reset-ignore-list \
jmyspell.unload-dictionary

jmyspell.spell-check.label = Check spelling
jmyspell.reset-ignore-list.label = Reset ignored words
jmyspell.unload-dictionary.label = Unload dictionary

## Error Messages
jmyspell.no-error.found = No spelling errors found
jmyspell.no-init = SpellChecker is not init.
jmyspell.error.dictionary.not-found = Error loading dictionary.\
\nBe sure the file exits and is in correct format (an OpenOffice.org dictionary)  
jmyspell.filter.description = Dictionary files (*.zip, *.dic, *.aff)


